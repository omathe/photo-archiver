<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.71" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CopyBoxController">
   <children>
      <Label fx:id="copyName" text="Label" />
      <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" />
      <Label fx:id="counter" />
      <Button fx:id="cancelCopy" mnemonicParsing="false" onAction="#cancelCopy" text="x" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
